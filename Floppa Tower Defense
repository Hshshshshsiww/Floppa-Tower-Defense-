game:GetService("StarterGui"):SetCore("SendNotification",{
Title = "üìÉScript  Floppa Tower Defense üìÉ",
Text = "create by üëëƒê·∫ø V∆∞∆°ng Th√°nh Gi·ªõi üëë", 
Duration = 3
})
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/DenDenZZZ/Kavo-UI-Library/main/Kavo.lua"))()

local Window = Library.CreateLib("floppa tower defense", "DarkTheme")

local Tab = Window:NewTab("interwebs T.d")

local Section = Tab:NewSection("Unit all")

Section:NewButton("AgentDog [cost 25$]", "ButtonInfo", function()
local player = game.Players.LocalPlayer  -- Define the player as the local player

local args = {
    [1] = "AgentDog",  -- Name of the tower
    [2] = CFrame.new(player.Character.HumanoidRootPart.Position) * CFrame.Angles(0, 0, 0)  -- Position and orientation of the tower at player's location
}

game:GetService("ReplicatedStorage").Functions.SpawnTower:InvokeServer(unpack(args))
    print("Clicked")
end)

